{
  "compilerOptions": {
    "incremental": true,
    "target": "es2018",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": false
  }
}
